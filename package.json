{
  "name": "@lit/lit-starter-js",
  "private": true,
  "version": "1.0.2",
  "description": "A simple web component",
  "type": "module",
  "keywords": [
    "web-components",
    "lit-element",
    "javascript",
    "lit"
  ],
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@ui5/webcomponents": "^1.2.1",
    "@ui5/webcomponents-fiori": "^1.2.1",
    "@ui5/webcomponents-icons": "^1.2.1",
    "@web/dev-server": "^0.1.22",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.3.9",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.10.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "eslint": "^7.32.0",
    "lit-element": "^3.2.0",
    "lit-element-router": "^2.0.3",
    "prettier": "^2.3.2",
    "rollup": "^2.70.1",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "customElements": "custom-elements.json",
  "scripts": {
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint '**/*.js'",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "start": "web-dev-server --watch",
    "server": "MODE=prod npm run serve",
    "build": "rollup -c"
